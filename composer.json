{
    "name": "spacekola/devups",
    "description": "a php framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Aurelien Atemkeng",
            "email": "aurelienatemkeng@spacekola.com",
            "role": "developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "doctrine/orm": "^2.1.3",
        "doctrine/cache": "1.11.3",
        "philo/laravel-blade": "3.*",
        "ifsnop/mysqldump-php": "^2.9",
        "phpmailer/phpmailer": "^6.6",
        "mpdf/mpdf": "^8.0",
        "guzzlehttp/guzzle": "^7.5",
        "maslosoft/addendum": "*",
        "firebase/php-jwt": "^6.9"
    },
    "autoload": {
        "classmap": [
            "admin/generator/",
            "dclass/",
            "src/"
        ],
        "psr-0": {"": "src/"}
    },
    "require-dev": {
        "symfony/var-dumper": "^5.4"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": false
        }
    }
}
